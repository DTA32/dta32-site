generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model article {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  category     String
  content      String
  date_created DateTime @db.Date
  image        String
  title        String
}

model contact {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  icon     String
  link     String
  platform String
  username String
}

model quote {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  content String
}

model song {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  artist   String
  imageUrl String
  link     String
  title    String
}
